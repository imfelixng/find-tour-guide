<!doctype html>
 <html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Travel Hub</title>
    <meta name="description" content="Ela Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="/css/style2.css">

</head>

<body>
   <!-- Left Panel -->
   <aside id="left-panel" class="left-panel">
    <nav class="navbar navbar-expand-sm navbar-default">
        <div id="main-menu" class="main-menu collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li class="active">
                    <a href="/admin"><i class="menu-icon fa fa-laptop"></i>Dashboard </a>
                </li>
                <li class="menu-title">Manager</li><!-- /.menu-title -->
                <li class="menu-item">
                    <a href="/admin/manage-user"> <i class="menu-icon fa fa-cogs"></i>Manager User</a>
                </li>
                <li class="menu-item">
                    <a href="/admin/manage-tourguide""> <i class="menu-icon fa fa-cogs"></i>Manager Tour Guide</a>
                </li>
                <li class="menu-item">
                    <a href="/admin/add-location"><i class="menu-icon fa fa-plus"></i>Add Location</a>
                </li>  
                <li class="menu-item">
                    <a href="/admin/manage-location"> <i class="menu-icon fa fa-cogs"></i>Manager Location</a>
                </li>                  
            </ul>
        </div><!-- /.navbar-collapse -->
    </nav>
</aside>
<!-- /#left-panel -->
    <!-- Right Panel -->
   
        
    <!-- Right Panel -->
    <div id="right-panel" class="right-panel">
        <!-- Content -->
        <div class="content">
                <!-- Orders -->
                <div class="card" id="manageuser">
                        <div class="card-body">
                            <h4 class="box-title">Manage User </h4>
                        </div>
                        <div class="card-body--">
                            <div class="table-stats order-table ov-h">
                                <table class="table ">
                                    <thead>
                                        <tr>
                                            <th class="serial">#</th>
                                            <th>ID</th>
                                            <th>Username</th>
                                            <th>Fullname</th>
                                            <th>Role</th>
                                            <th>Gender</th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(let i =0; i< users.length;i++){ %>
                                        <tr class=" pb-0">
                                            <td class="serial">5.</td>
                                            <td><%= users[i]._id%></td>
                                            <td>  <span ><%= users[i].username%></span> </td>
                                            <td><span><%= users[i].fullname%></span></td>
                                            <td><span ><%= users[i].role%></span></td>
                                            <td><span ><%= users[i].__v%></span></td>
                                            <td>
                                                <a href="/admin/manage-user/delete/<%= tourguides[i]._id%>" style="
                                                    color: red;
                                                ">Delete</a>
                                            </td>
                                        </tr>
                                        <%}%>
                                    </tbody>
                                </table>
                            </div> <!-- /.table-stats -->
                        </div>    
                    </div>                     
                    <!-- /.orders -->
                
        </div>
        <!-- /.content -->
        <div class="clearfix"></div>
    </div>
        <!-- /.content -->
        <div class="clearfix"></div>
    </div>
    <!-- /#right-panel -->
</body>
</html>
